<?xml version='1.0' encoding='utf-8'?>
<toolbarapplet on_load='initToolbar' on_click='onClickItem' buttonstyle='windows' buttonsize='26,25'>
	<toolbar id='main'>
		<button image="../images/Checkmark.svg" id="activate" tooltip="启用功能">启用功能</button>
		<button image="../images/Delete.svg" id="deactivate" tooltip="关闭功">关闭功能</button>
		<separator/>
		<button disabled="true" image="../images/PreviousPage.svg" id="prev_page" tooltip="上一页">上一页</button>
		<button disabled="false" image="../images/NextPage.svg" id="next_page" tooltip="下一页">下一页</button>
	</toolbar>
</toolbarapplet>