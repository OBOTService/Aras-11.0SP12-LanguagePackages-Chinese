<?xml version='1.0' encoding='utf-8'?>
<toolbarapplet on_load='initToolbar' on_click='onClickItem' buttonstyle='windows' buttonsize='26,25'>
	<toolbar id='main'>
		<button image="../images/Checkmark.svg" id="activate" tooltip="Activate feature">Activate feature</button>
		<button image="../images/Delete.svg" id="deactivate" tooltip="Deactivate feature">Deactivate feature</button>
		<separator/>
		<button disabled="true" image="../images/PreviousPage.svg" id="prev_page" tooltip="Previous Page">Previous Pages</button>
		<button disabled="false" image="../images/NextPage.svg" id="next_page" tooltip="Next Page">Next Page</button>
	</toolbar>
</toolbarapplet>