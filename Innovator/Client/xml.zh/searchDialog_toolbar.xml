<?xml version="1.0" encoding="utf-8"?>
<toolbarapplet buttonsize="26,25">
  <toolbar id="search">
    <choice id="implementation_type" invisible="true"/>
    <button image="../images/ExecuteSearch.svg" id="search" tooltip="执行搜索" text="搜索"/>
    <button image="../images/ClearSearchCriteria.svg" id="newsearch" tooltip="清除搜索条件" text="清除条件"/>
    <button disabled="true" image="../images/StopSearch.svg" id="stop_search" tooltip="停止搜索" text="停止搜索" />
    <separator/>
    <choice width="auto"  id="search_mode" tooltip="搜索模式" text="搜索模式">
    </choice>
    <choice id="saved_search" tooltip="储存搜索" text="储存搜索" label="储存搜索:">
    </choice>
    <separator/>
    <button image="../images/AddSearchCriteria.svg" id="add_criteria" tooltip="加入搜索条件" text="加入条件" invisible="true" />
    <button image="../images/SelectAll.svg" id="select_all" tooltip="全选" text="全选"/>
    <edit id="page_size" tooltip="Page Size:" text="" size="3"/>
    <button disabled="true" image="../images/PreviousPage.svg" id="prev_page" tooltip="上一页" text="上一页"/>
    <button disabled="true" image="../images/NextPage.svg" id="next_page" tooltip="下一页" text="下一页"/>
    <separator/>
    <edit id="max_search" tooltip="最大搜索条数:" text="" size="3"/>
    <separator/>
    <button image="../images/Checkmark.svg" id="ready_btn" tooltip="选择" text="选择"/>
    <button image="../images/Delete.svg" id="set_nothing" tooltip="清除" text="清除"/>
  </toolbar>
</toolbarapplet>
