<?xml version="1.0" encoding="utf-8"?>
<toolbarapplet on_load="onToolbarAppletLoad" on_click="onClickItem" buttonstyle="windows" buttonsize="26,25">
	<toolbar id="toolbar">
		<button image="../images/ExecuteSearch.svg" id="search" tooltip="Run search">Run search</button>
		<separator/>
		<edit id="page_size" tooltip="Page Size" text="25" size="3"/>
		<button disabled="true" image="../images/PreviousPage.svg" id="prev_page" tooltip="Previous Page" text="Previous Page"/>
		<button disabled="true" image="../images/NextPage.svg" id="next_page" tooltip="Next Page" text="Next Page"/>
		<separator/>
		<button image="../images/AddUser.svg" id="new" tooltip="Create trucore user">Create trucore user</button>
		<button image="../images/DisableUser.svg" id="disable" tooltip="Disable trucore user">Disable trucore user</button>
		<button image="../images/EnableUser.svg" id="enable" tooltip="Enable trucore user">Enable trucore user</button>
		<button image="../images/Checkmark.svg" id="pick" tooltip="Pick trucore user">Pick trucore user</button>
	</toolbar>
</toolbarapplet>
